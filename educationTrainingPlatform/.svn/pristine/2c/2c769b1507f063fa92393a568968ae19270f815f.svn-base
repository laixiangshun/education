<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>

<html>
  <head>
  	<title>personalCourseImport</title>
  	<style type="text/css">
  		.personalCourseImport p{
  			display: inline-block;
  			text-align: left;
  		}
  		.personalCourseImport .form-control{
  			width: 250px;
  		}
  		.personalCourseImport .importCondition{
  			text-align: left;
  			margin-left: 20%;
  		}
  		.personalCourseImport .importTitle{
  			text-align: left;
  			margin-left: 17%;
  		}
  		.personalCourseImport .importTitle p{
  			padding-left:5px;
  			color:white;
  			width:80px;
  			background-color: #337AB7;
  		}
  	</style>
  </head>
  
  <body>
  	<div class="personalCourseImport">
	    <div class="importTitle">
	    	<p><strong> 匯入</strong></p>
	    </div>
	    <div class="importCondition">
	    	<p>模板下載：</p><a>學員資料模板</a>
	    </div>
	    <form class="importCondition" action="" enctype="multipart/form-data" >
		    <div id="files">
			    <p>學員：</p><input class="form-control" type="file" name="fileupload"/>
		    </div>
	    </form>
    </div>
  </body>
</html>
